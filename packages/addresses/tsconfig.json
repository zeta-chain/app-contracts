{
  "compilerOptions": {
    "typeRoots": ["@types", "./node_modules/@types"],
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "baseUrl": "./",
    "declaration": true,
    "module": "commonjs",
    "resolveJsonModule": true,
  }
}
